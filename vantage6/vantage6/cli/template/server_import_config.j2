organizations:
{% for org in organizations %}
- name: {{ org['name'] }}
  domain: iknl.nl
  address1: Godebaldkwartier 419
  address2:
  zipcode: 3511DT
  country: Netherlands
  {% if org['make_admin'] %}
  users:
    - username: dev_admin
      firstname: admin
      lastname: robot
      password: password
  {% endif %}
  public_key: 
{% endfor %}
collaborations:
- name: {{ collaboration['name'] }}
  domain: iknl.nl
  address1: Godebaldkwartier 419
  address2:
  zipcode: 3511DT
  country: Netherlands
  participants:
  {% if collaboration['participants'] %}
  {% for party in collaboration['participants'] %}
    - name: {{ party['name'] }}
      api_key: {{ party['api_key'] }}
  {% endfor %}
  {% endif %}
  tasks: ['hello-world']
  encrypted: false
